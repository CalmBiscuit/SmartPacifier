/*ALARM numbers:
 * 1- High temp
 * 2- Low temp
 * 3-High HR
 * 4-Low HR
 * 5-High Oxygen
 * 6- Low oxygen
 */

int frame = 0;
void caution(int alarm) {
  while (buttonState != HIGH){
  display.clearDisplay();
  display.drawBitmap(40, 8, frames[frame], FRAME_WIDTH, FRAME_HEIGHT, 1);
  display.display();
  frame = (frame + 1) % FRAME_COUNT;
  delay(FRAME_DELAY);
  Serial.println(buttonState);
  return;
  }

//  display.clearDisplay();
//  switch (alarm) {
//  case 1:
//    display.setCursor(30,18);                
//    display.println("HIGH TEMP "); 
//    break;
//  case 2:
//    display.setCursor(30,18);                
//    display.println("LOW TEMP "); 
//    break;
//  case 3:
//    display.setCursor(30,18);                
//    display.println("HIGH BPM "); 
//    break;
//  case 4:
//    display.setCursor(30,18);                
//    display.println("LOW BPM "); 
//    break;
//  case 5:
//    display.setCursor(30,18);                
//    display.println("HIGH O2 ");
//    break;
//  case 6:
//    display.setCursor(30,18);                
//    display.println("LOW O2 ");
//    break;
//  default:
//    display.setCursor(30,18);                
//    display.println("CAUTION!");
}
